(() => {\n    window.addEventListener('scroll', changeScrollNav)\n  }, [])\n\n  return (\n    <>\n      <MenuToggle isOpen={isOpen} toggle={toggle} />\n      <Container scrollNav={scrollNav}>\n        <Wrapper>\n          <button\n            onClick={toggleSidebar}\n            className=\"btn btn-link d-md-none rounded-circle mr-3\"\n          >\n            <FaBars />\n          </button>\n          <Logo>NS software</Logo>\n\n          <MobileIcon onClick={toggle}>\n            <FaBars />\n          </MobileIcon>\n\n          <NavMenu isOpenJWT={isOpenJWT}>\n            <li className=\"nav-item dropdown no-arrow mx-1\">\n              <a\n                className=\"nav-link dropdown-toggle\"\n                href=\"#\"\n                role=\"button\"\n                aria-haspopup=\"true\"\n              >\n                vagas\n              </a>\n            </li>\n            <li className=\"nav-item dropdown no-arrow mx-1\">\n              <a\n                className=\"nav-link dropdown-toggle\"\n                href=\"#\"\n                role=\"button\"\n                aria-haspopup=\"true\"\n              >\n                testes\n              </a>\n            </li>\n            <li className=\"nav-item dropdown no-arrow mx-1\">\n              <a\n                className=\"nav-link dropdown-toggle\"\n                href=\"#\"\n                role=\"button\"\n                aria-haspopup=\"true\"\n              >\n                perfil\n              </a>\n            </li>\n          </NavMenu>\n        </Wrapper>\n      </Container>\n    </>\n  )\n}\n\nexport default navbar\n","/home/ricardo/desenvolvimento/portifolio/ns/client/src/components/login/index.tsx",[],"/home/ricardo/desenvolvimento/portifolio/ns/client/src/components/login/styles.ts",[],[],"/home/ricardo/desenvolvimento/portifolio/ns/client/src/reportWebVitals.ts",[],"/home/ricardo/desenvolvimento/portifolio/ns/client/src/pages/dados/styles.ts",[],"/home/ricardo/desenvolvimento/