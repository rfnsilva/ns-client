n      const userUpdate = {\n        name: inputName.current.value,\n        email: inputEmail.current.value,\n        surname: inputSurname.current.value,\n        github: inputGithub.current.value,\n        linkedin: inputLinkedin.current.value,\n        behance: inputBehance.current.value,\n        phone: inputPhone.current.value\n      }\n\n      await updateUser(userUpdate, user.id)\n    }\n  }\n\n  return (\n    <Container isOpenSidebar={isOpenSidebar}>\n      <div className=\"container-fluid px-1 px-md-4 py-5 mx-auto\">\n        <div\n          className=\"row d-flex justify-content-center px-3\"\n          style={{ margin: '0' }}\n        >\n          <div className=\"card\">\n            <div className=\"row\">\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12 titleSection\">\n                <h2 className=\"\">Foto de perfil</h2>\n                <p className=\"\">Adicione uma foto em seu perfil</p>\n              </div>\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12\">\n                <div className=\"mx-auto mb-3 text-left\">\n                  <img className=\"profile-img\" src={user.image} />\n                </div>\n                <div>\n                  <div className=\"input-upload\">\n                    <label className=\"input-upload-label\" htmlFor=\"input-file\">\n                      Carregar foto\n                    </label>\n                    <input\n                      type=\"file\"\n                      id=\"input-file\"\n                      className=\"btn btn-primary\"\n                      onChange={handleChangeImage}\n                      style={{ display: 'none' }}\n                    />\n                    {/* <button type=\"button\" className=\"btn btn-primary\">\n                      carregar\n                    </button> */}\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"card alt\">\n            <div className=\"row\">\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12\">\n                <h2 className=\"\">Email</h2>\n                <p className=\"\">Você pode alterar seu email</p>\n              </div>\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12\">\n                <div className=\"mt-3 mx-auto  mb-3 text-left\">\n                  <div className=\"form-element-component\">\n                    <label className=\"label\">\n                      Email\n                      <strong className=\"stars\">*</strong>\n                    </label>\n                    <input\n                      className=\"input\"\n                      defaultValue={\n                        user.email === null || user.email === undefined\n                          ? ''\n                          : user.email\n                      }\n                      placeholder=\"email@exemplo.com\"\n                      type=\"text\"\n                      name=\"email\"\n                      ref={inputEmail}\n                    />\n                  </div>\n                </div>\n                <button\n                  onClick={submitEmailUpdate}\n                  className=\"btn btn-primary\"\n                  type=\"button\"\n                >\n                  <span>Atualizar email</span>\n                </button>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"card\">\n            <div className=\"row\">\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12\">\n                <h2 className=\"\">Instruções</h2>\n                <p className=\"\">\n                  Preencha seus dados de perfil. Sempre mantenha seu telefone\n                  atualizado.\n                </p>\n              </div>\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12\">\n                <div className=\"form-element-component\">\n                  <label className=\"label\">\n                    Nome\n                    <strong className=\"stars\">*</strong>\n                  </label>\n                  <input\n                    className=\"input\"\n                    placeholder=\"Digite seu nome\"\n                    type=\"text\"\n                    name=\"name\"\n                    defaultValue={\n                      user.name === null || user.name === undefined\n                        ? ''\n                        : user.name\n                    }\n                    ref={inputName}\n                  />\n                </div>\n                <div className=\"form-element-component\">\n                  <label className=\"label\">\n                    Sobrenome\n                    <strong className=\"stars\">*</strong>\n                  </label>\n                  <input\n                    className=\"input\"\n                    placeholder=\"Digite seu sobrenome\"\n                    type=\"text\"\n                    name=\"surname\"\n                    defaultValue={\n                      user.surname === null || user.surname === undefined\n                        ? ''\n                        : user.surname\n                    }\n                    ref={inputSurname}\n                  />\n                </div>\n                <div className=\"form-element-component\">\n                  <label className=\"label\">\n                    Telefone\n                    <strong className=\"stars\">*</strong>\n                  </label>\n                  <input\n                    className=\"input\"\n                    placeholder=\"(00) 00000-0000\"\n                    type=\"text\"\n                    name=\"phone\"\n                    defaultValue={\n                      user.phone === null || user.phone === undefined\n                        ? ''\n                        : user.phone\n                    }\n                    ref={inputPhone}\n                  />\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div className=\"card\">\n            <div className=\"row\">\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12\">\n                <h2 className=\"\">Links Pessoais</h2>\n                <p className=\"\">\n                  Compartilhe seu perfil de outras plataformas aqui.\n                </p>\n              </div>\n              <div className=\"col-sm-12 col-lg-6 col-md-6 col-12\">\n                <div className=\"form-element-component\">\n                  <label className=\"label\">Github</label>\n                  <input\n                    className=\"input\"\n                    placeholder=\"https://github.com/abcdefghi\"\n                    type=\"text\"\n                    name=\"github\"\n                    defaultValue={\n                      user.github === null || user.github === undefined\n                        ? ''\n                        : user.github\n                    }\n                    ref={inputGithub}\n                  />\n                </div>\n                <div className=\"form-element-component\">\n                  <label className=\"label\">Behance</label>\n                  <input\n                    className=\"input\"\n                    placeholder=\"https://www.behance.net/abcdefghi\"\n                    type=\"text\"\n                    name=\"behance\"\n                    defaultValue={\n                      user.behance === null || user.behance === undefined\n                        ? ''\n                        : user.behance\n                    }\n                    ref={inputBehance}\n                  />\n                </div>\n                <div className=\"form-element-component\">\n                  <label className=\"label\">Linkedin</label>\n                  <input\n                    className=\"input\"\n                    placeholder=\"https://www.linkedin.com/in/abcdefghi\"\n                    type=\"text\"\n                    name=\"linkedin\"\n                    defaultValue={\n                      user.linkedin === null || user.linkedin === undefined\n                        ? ''\n                        : user.linkedin\n                    }\n                    ref={inputLinkedin}\n                  />\n                </div>\n              </div>\n            </div>\n          </div>\n          <div className=\"col-lg-10 p-3 d-flex justify-content-center\">\n            <button type=\"button\" onClick={submitUpdateUser}>\n              ATUALIZAR\n            </button>\n          </div>\n        </div>\n      </div>\n    </Container>\n  )\n}\nexport default layoutdados\n","/home/ricardo/desenvolvimento/portifolio/ns/client/src/components/layoutdados/styles.ts",[],"/home/ricardo/desenvolvimento/portifolio/ns/client/src/routes/private.tsx",[],"/home/ricardo/desenvolvimento/portifolio/ns/client/src/components/sidebar/styles.ts",[],"/home/ricardo/desenvolvimento/portifolio/ns/client/src/components/sidebar/index.tsx",["105","106","107"],{"ruleId":"108","severity":1,"message":"109","line":14,"column":21,"nodeType":"110","messageId":"111","endLine":14,"endColumn":33},{"ruleId":"108","severity":1,"message":"112","line":1,"column":29,"nodeType":"110","messageId":"111","endLine":1,"endColumn":37},{"ruleId":"108","severity":1,"message":"113","line":2,"column":44,"nodeType":"110","messageId":"111","endLine":2,"endColumn":56},{"ruleId":"108","severity":1,"message":"114","line":4,"column":21,"nodeType":"110","messageId":"111","endLine":4,"endColumn":25},{"ruleId":"108","severity":1,"message":"115","line":11,"column":32,"nodeType":"110","messageId":"111","endLine":11,"endColumn":55},"@typescript-eslint/no-unused-vars","'setIsOpenJWT' is assigned a value but never used.","Identifier","unusedVar","'useState' is defined but never used.","'FaAngleRight' is defined but never used.","'Icon' is defined but never used.","'setIsOpenSidebarSubMenu' is assigned a value